<!DOCTYPE html>
<html>
<head>
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
	<link rel="stylesheet" href="https://unpkg.com/purecss@1.0.0/build/pure-min.css" integrity="sha384-nn4HPE8lTHyVtfCBi5yW9d20FjT8BJwUXyWZT9InLYax14RDjBj46LmSztkmNP9w" crossorigin="anonymous">
	<meta charset="utf-8"/>
	<!--[if lte IE 8]>
    <link rel="stylesheet" href="https://unpkg.com/purecss@1.0.0/build/grids-responsive-old-ie-min.css">
<![endif]-->
<!--[if gt IE 8]><!-->
    <link rel="stylesheet" href="https://unpkg.com/purecss@1.0.0/build/grids-responsive-min.css">
<!--<![endif]-->
	<link rel="stylesheet" href="/styles/style.css">
	<link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro" rel="stylesheet">
    <script src="/jquery/jquery-ui-1.12.1.custom/jquery-ui-1.12.1.custom/jquery-ui.js"></script>
    <link rel="stylesheet" href="/jquery/jquery-ui-1.12.1.custom/jquery-ui-1.12.1.custom/jquery-ui.css">
	<script>
	$(document).ready(function(){
	$('#password').click(function(){
		$("#sub").css("opacity", "0");
		});
	});
	</script>
</head>
<script src='../expanding.jquery.js'></script>
<body>
	<section>
		<div class="pure-g">
			<div class="pure-u-1" id="header">
				<img src="/images/backgroundDefault.jpg" style="top: -80px; position:relative;  z-index: -1;">

			</div>
		</div>
		<div id="excludingHeader">
			<div class="pure-g">
				<div class="pure-u-lg-1-5" id="sidebar"> </div>
					<div class="pure-u-1 pure-u-lg-3-5">
						<div id="formArea">
						<div id="formTitle"> Form 7 - Planning </div>
						<br><br>
                        <form name="form7" class="pure-form" action="" method="post" onsubmit="return validateForm()">
						<!--<form class="pure-form">
-->						<div class="sectionContainer">
								<div class="sectionTitle"> Fieldwork Description </div>
								<div class="input">
								<fieldset>
                                    <strong>Unit of study / Research project / Other title:</strong> <br>
									<%=form7.projectTitle%>
									<br> <br>
                                        <h3>Date of Fieldwork:</h3>
									<table style="width:100%">
										<tr>
											<td>
                                                <strong>Start Date:</strong> 
											<%=form7.startDate%>
											</td>
											<td>
                                                <strong>End Date: </strong>
											<%=form7.endDate%>
											</td>
										</tr>
									</table>
									<br><br>
									
                                    <strong>Fieldwork Description: </strong><br>
									<textarea name = "fieldDescription" id = "fieldDescription" class = "expanding"
									readonly style="border: none; background-color:#FFF !important;  color: black !important;"><%=form7.fieldDescription%> </textarea>
									<br><br>
									<br>
									
									
								</fieldset>
								</div>
							</div>
						<!--</form>
-->
						<br><br>
						
						
						<!--<form class="pure-form">
-->							<div class="sectionContainer">
								<div class="sectionTitle"> Emergency UWA Contact and Call- Home Arrangements </div>
								<div class="input">
								<fieldset>
								
									<table id = "UWAContact" style = "width:100%">
										<thead>
										<tr>
											<td>
                                                <strong>UWA Contact Name(s)</strong>
											</td>
											<td>
                                                <strong>Phone Number(s)</strong>
											</td>
										</tr>
										</thead>
										
										<tbody>
                                        <%for (var i = 0; i < form7.contactName.length;i++) {%>
										<tr>
											<td>
												<%=form7.contactName[i]%>
											</td>
											<td>
												<%=form7.contactPhone[i]%>
											</td>
										</tr>
                                        <% } %>
										</tbody>
									</table>
									<br>
									<br>
									
                                    <strong>Call-Home Arrangement:</strong> <br>
									<textarea name = "callHome" id="callHome" class = "expanding"
									readonly style="border: none; background-color:#FFF !important; color: black !important;"><%=form7.callHome%></textarea>
									<br>
                                    <br>
									
                                    <strong>Emergency response plan if call-home arrangements fail:</strong> <br>
									<textarea name = "emergency" id="emergency" class = "expanding"
									readonly style="border: none; background-color:#FFF !important;  color: black !important;"><%=form7.emergency%></textarea>
									<br>
									<br>
								</fieldset>
								</div>
							</div>
							<!--</form>
-->
							<br><br>
							
							<!--<form class="pure-form">
-->							<div class="sectionContainer">
								<div class="sectionTitle"> Transport Arrangements To Fieldwork Site(s) </div>
								<div class="input">
								<fieldset> <br>
									<textarea name = "transport" id="transport" class = "expanding"
									readonly style="border: none; background-color:#FFF !important;  color: black !important;"><%=form7.transport%></textarea>
									
								</fieldset>
								</div>
							</div>
							<!--</form>
-->
							<br><br>
							
							
							<!--<form class="pure-form">
-->							<div class="sectionContainer">
								<div class="sectionTitle"> Itinerary Details </div>
								<div class="input">
								<fieldset>
									<table id = "itin" style = "width:100%">
										<thead>
										<tr>
                                            <td> <strong>Date </strong> </td> 
                                            <td> <strong>Location </strong></td>
                                            <td> <strong>Accommodation </strong></td>
                                            <td> <strong>Contact Information </strong></td>
										</tr>
										</thead>
										<tbody>
										<%for(var i = 0;i < form7.itinDate.length;i++) {%>
										  <tr>
                                            <td><%=form7.itinDate[i]%></td>
											<td> <%=form7.itinLocation[i]%></td>
											<td> <%=form7.itinAccomodation[i]%></td>
											<td> <%=form7.itinPhone[i]%> </td>
										</tr>
										<% } %>
										</tbody>
									</table>
                                    <br>
								</fieldset>
								</div>
							</div>
							<!--</form>
-->
							<br><br>
							
							<!--<form class="pure-form">
-->							<div class="sectionContainer">
								<div class="sectionTitle"> Participants Declaration </div>
								<div class="input">
								<fieldset>
									<table id = "declaration">
										<thead>
										<tr>
                                            <td> <strong>Name</strong> </td> 
                                            <td> <strong>Phone Number (during field trip)</strong> </td>
                                            <td> <strong>Volunteer / Collaborator</strong> </td>
                                            <td> <strong>Driver Competence:</strong> <br> 
											Approved Driver / 4WD Trained? / Off-Road Trained? </td>
                                            <td> <strong>First Aid Certified</strong> </td>
										</tr>
										</thead>
										<tbody>
										<%for(var i = 0;i < form7.decName.length;i++) {%>
										<tr>
											<td><%=form7.decName[i]%> </td>
											<td><%=form7.decPhone[i]%> </td>
											<td> <label for="decVolunteer1">
                                                    <input type="hidden" name="decVolunteer" value="false">
													<%if (form7.decVolunteer[i] == 'decVolunteer') {%>
													<input type="checkbox" id="decVolunteer1" name="decVolunteer" value="decVolunteer" checked onclick="return false;"> V
													<% }else  {%>
													<input type="checkbox" id="decVolunteer1" name="decVolunteer" value="decVolunteer" onclick="return false;"> V
													<% } %>
                                                </label>
												<label for="decCollaborator1">
                                                    <input type="hidden" name="decCollaborator" value="false">
													<%if (form7.decCollaborator[i] == "decCollaborator") {%>
													<input type="checkbox" id="decCollaborator1" name="decCollaborator" value="decCollaborator" checked onclick="return false;"> C
													<%} else  {%>
													<input type="checkbox" id="decCollaborator1" name="decCollaborator" value="decCollaborator" onclick="return false;"> C
													<% } %>
                                                </label>
											</td>
											<td> <label for="decApproved1">
                                                    <input type="hidden" name="decApproved" value="false">
													<%if (form7.decApproved[i] == "decApproved") {%>
													AP <input type="checkbox" id="decApproved1" name="decApproved" value="decApproved" checked onclick="return false;"> 
													<%} else  {%>
													AP <input type="checkbox" id="decApproved1" name="decApproved" value="decApproved" onclick="return false;"> 
													<% } %>
												</label>
												
												<label for="dec4WD1">
                                                    <input type="hidden" name="dec4WD1" value="false">
													<%if (form7.dec4WD[i] == "dec4WD") {%>
													4WD <input type="checkbox" id="dec4WD1" name="dec4WD" value="dec4WD" checked onclick="return false;">  
													<%} else  {%>
													4WD <input type="checkbox" id="dec4WD1" name="dec4WD" value="dec4WD" onclick="return false;">  
													<% } %>
												</label>
												
												<label for="decOffRoad1">
                                                    <input type="hidden" name="decOffRoad" value="false">
													<%if (form7.decOffRoad[i] == "decOffRoad") {%>
													OR <input type="checkbox" id="decOffRoad1" name="decOffRoad" value="decOffRoad" checked onclick="return false;"> 
													<%} else  {%>
													OR <input type="checkbox" id="decOffRoad1" name="decOffRoad" value="decOffRoad" onclick="return false;"> 
													<% } %>
												</label>
											</td>
											<td> <label for="decAid1">
                                                <input type="hidden" name="decAid" value="false">
												<%if (form7.decAid[i] == "decAid") {%>
													<input type="checkbox" id="decAid1" name="decAid" value="decAid" checked onclick="return false;"> 
													<%} else  {%>
													<input type="checkbox" id="decAid1" name="decAid" value="decAid" onclick="return false;">
													<% } %>
												</label>
											</td>
										</tr>
										<% } %>
										</tbody>
									</table>
									<br>
									
									<label for="agreement">
										<input type="checkbox" id="agreement" name="agreement" value="agreement" checked onclick="return false;"> I have read 
										and fully understand the requirements of this Fieldwork Safety Plan and associated documents. 
										I have informed all participants on this fieldwork trip of the information on this form. All necessary forms (excluding this one)
										have been completed and submitted. Field plans have been distributed to each member of this fieldwork
										trip for record keeping.<br> 
                                        <label>
                                            <%if (form7.riskformcheck != null) {%>
													<input type="checkbox" id="riskform" name="riskform" value="riskform" checked disabled> If you have already completed a risk assessment form that is valid for this fieldtrip please check the box. <br>
													<% } else  {%>
													<input type="checkbox" id="riskform" name="riskform" value="riskform" disabled> If you have already completed a risk assessment form that is valid for this fieldtrip please check the box.<br>
				                    <% } %></label><br>
									</label>
									<br>
								</fieldset>
								</div>
							</div>
							<!--</form>-->
							<br><br>
							
							
							
							<div class="sectionContainer">
								<div class="sectionTitle"> Permission to Proceed</div>
								<div class="input">
								<fieldset>
									<br>
									<table style="width:100%">
										<thead>
										<tr>
											<td>
												
											</td>
											<td>
                                                <strong>Name</strong>
											</td>
											<td>
                                                <strong>Date</strong>
											</td>
										</tr>
										</thead>
										
										<tbody>
										<tr>
                                            </tr>
										<tr>
											<td>
                                                <strong>Fieldwork Leader:</strong>
											</td>
											<td>
											<%=form7.leaderName%>
											</td>
											<td>
											<%=form7.leaderDate%>
											</td>
										</tr>
										<tr>
											<td>
                                                <strong>Fieldwork Supervisor</strong>
											</td>
											<td>
											<%=form7.supervisorName%>
											</td>
											<td>
											<%=form7.supervisorDate%>
											</td>
										</tr>
                                        <tr>
                                            <td><strong>Fieldwork Coordinator:</strong></td>
											<td><%=form7.fcname%></td>
                                            <td><%=form7.fcdate%></td>
                                        </tr>
										<tr>
											<td>
                                                <strong>Head of School:</strong>
											</td>
											<td>
											<%=form7.hosName%>
											</td>
											<td>
											<%=form7.hosDate%>
											</td>
										</tr>
										</tbody>
									</table>
									<br>
                                    <label>
                                        <%if (form7.DBSOcheck != null) {%>
													<input type="checkbox" id="DBSO" name="DBSO" value="DBSO" checked disabled> If any snorkelling, diving or boat is involved on this trip, approval from DSBO is required. I have attached approval from the DBSO. <br>
													<% }else  {%>
													<input type="checkbox" id="DBSO" name="DBSO" value="DBSO" disabled> If any snorkelling, diving or boat is involved on this trip, approval from DSBO is required. I have attached approval from the DBSO. <br>
				                    <% } %>
                                    </label>
									<label for="permission">
										<input type="checkbox" id="permission" name="permission" value="permission" checked disabled> I have read 
										this form and give my permission to proceed with fieldwork<br>  
									</label><br>
								</fieldset>
								</div>
							</div>
						</form>
						<br>
						<form action="printForm7" method="post">
							<input type = "hidden" value = "<%=form7._id%>" name = "fid">
							<input type="submit" value = "print" style = "opacity:100" id = "sub">
						</form>
						<input type = "button" hidden style = "pointer-events: none" id = "password">
						</div>
						</div>
				<div class="pure-u-lg-1-5" id="sidebar"> </div>
			</div>
		</div>
		<br>
	</section>
</body>



