		var admins = [];
		<% for (var i = 0; i < admins.length; i ++){ %>
			admins.push(<%- JSON.stringify(admins[i].username) %>);
		<% } %>
		
		function checkAdmin(id){
			var em = $(id).val();
			for (var k = 0; k < admins.length; k++){
				if (admins[k] == em){
					return true;
				}
			}
			alert(em + " does not match any emails");
			return false;
		}
	

